
class calculator:

     def addition(x,y):
         added = x + y
         print(added)

     def substraction(x,y):
         sub = x - y
         print(sub)

     def multiplication(x,y):
          mult = x * y
          print(mult)

     def division(x,y):
           mult = x / y
           print(div)
           
calculator.multiplication(3,5)
calculator.addition(2,6)
calculator.substraction(8,4)
calculator.division(10,2)


